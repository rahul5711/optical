                <% if(data.paidlist.length !==0){
                  data.paidlist.forEach(function (i, index) { %>
                  <% if (i.PaymentMode !=='Payment Initiated' ){%>
                      <span style="font-size: 10px;">
                          <%=i.PaymentMode%>/
                      </span>
                      <%}%>
                          <%})}%>
              </td>
            </tr>
          </table>
        </div>
      </div>

  </section>

  <section class="Signature">
    <div class="row">
      <div class="col1">
