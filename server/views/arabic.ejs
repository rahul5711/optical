<style>
  html body {
    zoom: 150%;
  }

  .header {
    width: 100%;
    height: 115px;
  }

  .row .col1 {
    width: 32%;
    float: left;
  }

  .row .col1 p {
    font-size: 10px;
    margin: 0;
    line-height: 13px;
  }

  .row .col2 {
    width: 32%;
    float: left;
    text-align: center;
  }

  .row .col3 {
    width: 32%;
    float: right;
    padding-right: 0px;
  }

  .row .col3 p {
    font-size: 10px;
    margin: 0;
    text-align: right;
    padding-right: 10px;
    line-height: 13px;
  }

  .row2 {
    height: 15px;
    width: 100%;
    text-align: center;
    border: 1px solid;
  }

  .row2 .col1 p {
    width: 100%;
    font-size: 12px;
    text-align: center;
    font-weight: 600;
  }

  .row3 {
    height: 50px;
    width: 100%;
  }

  .row3 .col1 {
    width: 50%;
    float: left;
  }

  .row3 .col1 .col1_11 {
    width: 33%;
    float: left;
  }

  .row3 .col1 .col1_12 {
    width: 65%;
    float: right;
    text-align: left;
  }

  .row3 .col1 p {
    font-size: 10px;
    line-height: 15px;
    margin: 0;
  }

  .row3 .col2 {
    float: right;
    width: 35%;
  }

  .row3 .col2 .col1_11 {
    width: 47%;
    float: left;
  }

  .row3 .col2 .col1_12 {
    width: 53%;
    float: right;
    text-align: left;
  }

  .row3 .col2 p {
    font-size: 10px;
    line-height: 15px;
    margin: 0;
  }

  .maintbl table thead tr th {
    font-size: 9px;
  }

  .maintbl table tbody tr td {
    font-size: 9px;
    text-align: center;
  }

  tr:nth-child(even) {
    background-color: #f2f2f2;
  }

  .totals .row {
    width: 100%;
    height: 100px;
  }

  .totals .row .col1 {
    width: 50%;
    float: left;
  }

  .totals .row .col1 p {
    font-size: 11px;
    margin: 0;
  }

  .totals .row .col2 {
    width: 45%;
    float: right;
  }

  .totals .row .col2 p {
    font-size: 11px;
    margin: 0;
  }

  .totals .row .col2 .col2_11 table {
    width: 100%;
    float: left;
    text-align: left;
    font-size: 11px;
    font-weight: normal;

  }

  .totals .row .col2 .col2_11 table tr th {
    text-align: left;
    border: 1px solid #eaeaea;
    font-weight: normal;
  }

  .totals .row .col2 .col2_11 table tr td {
    text-align: right;
    border: 1px solid #eaeaea;
    font-weight: normal;
  }

  .totals .row .col2 .col2_11 table tr:nth-child(even) {
    background-color: #ffffff;
  }

  .Signature .row {
    width: 100%;
    height: 50px;
  }

  .Signature .row .col1 {
    width: 40%;
    float: left;
  }

  .Signature .row .col2 {
    width: 40%;
    float: right;
    text-align: right;
  }

  .Signature .row p {
    font-size: 11px;
    margin: 0;
  }
</style>


