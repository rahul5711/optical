                    <% if (data.GSTTypes=="CGST-SGST" ) { %>
                      <th>CGST</th>
                      <th>SGST</th>
                      <%} else if (data.GSTTypes=='IGST' ) { %>
                        <th>IGST</th>
                        <% } else if (data.GSTTypes=='VAT' ) { %>
                          <th>VAT</th>
                          <% } else { %>
                            <th>None</th>
                            <% } %>
                              <%}%>
                                <th>TotalOMR <br> الإجمالي ريال عماني</th>
        </tr>
      </thead>
      <tbody>
        <% if(data.billItemList.length !==0 ){ data.billItemList.forEach(function (i, index) { %>
          <tr>
            <td>
              <%= index+1 %>
            </td>
           
                <td style="text-align: left;">
                  <%=i.ProductName%>
                    <% if (i.Remark !=='undefined' ){%>
                      <%=i.Remark%>/ <%}%>
                          <% if (i.Warranty !=='undefined' ){%>
                            <%=i.Warranty%>
                              <%}%>
                </td>
   <% if (data.shopdetails.Rate==="true" ){%>
                  <td>
                    <%=new Intl.NumberFormat(data.currencyLocale,{currency:data.currencyFormat}).format(i.UnitPrice)%>
                  </td>
                  <%}%>
                <td>
                  <%=i.Quantity%>
                </td>
             
                    <% if (data.shopdetails.Discounts==="true" ){%>
                      <td>
                        <%=new
                          Intl.NumberFormat(data.currencyLocale,{currency:data.currencyFormat}).format(i.DiscountAmount)%>
                      </td>
                      <%}%>
                        <% if (data.shopdetails.SubTotal==="true" ){%>
                          <td>
                            <%= i.SubTotal %>
                          </td>
                          <%}%>
                            <% if (data.shopdetails.Tax==="true" && data.bill==='Tax Invoice' ){%>

                              <% if (data.GSTTypes=="CGST-SGST" ) { %>
                                <td>
                                  <%= i.GSTAmount %> <br> @ <%= i.GSTPercentage %>
                                </td>
                                <td>
                                  <%= i.GSTAmount %> <br> @ <%= i.GSTPercentage %>
                                </td>
                                <%} else { %>
                                  <td>
                                    <%= i.GSTAmount %> <br> @ <%= i.GSTPercentage %>
                                  </td>
                                  <% } %>
                                    <%}%>

                                      <% if (data.shopdetails.Total==="true" ){%>
                                        <td>
                                          <%= i.TotalAmount %>
                                        </td>
                                        <%}%>
          </tr>

          <%})}%>
            <% if(data.serviceList !==null && data.serviceList.length !==0){ data.serviceList.forEach(function (i,
              index) { %>
              <tr>
                <td>
                  <%= index+1 %>
                </td>
               

                    <td>
                      <%= i.ServiceType %>/<%= i.Description %>
                    </td>
                    

                    <% if (data.shopdetails.Rate==="true" ){%>
                      <td>
                        <%= i.Price %>
                      </td>
                      <%}%>
                      <td> </td>
                        <% if (data.shopdetails.Discounts==="true" ){%>
                          <td> </td>
                          <%}%>

                            <% if (data.shopdetails.Discounts==="true" ){%>
                              <td>
                                <%= i.SubTotal %>
                              </td>
                              <%}%>


                                <% if (data.shopdetails.Tax==="true" && data.bill==='Tax Invoice' ){%>

                                  <% if (data.GSTTypes=="CGST-SGST" ) { %>
