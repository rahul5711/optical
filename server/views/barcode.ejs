                                        <div
                                            style="text-align: left; font-size: 20px; font-weight: 600; font-family: verdana; letter-spacing: 0px; margin-left: 15px; height: 22px;">
                                            MRP
                                            <%=i.RetailPrice%>
                                        </div>
                                        <div
                                            style="text-align: left; font-size: 12px; font-weight: 600;letter-spacing: 0.1px;margin-left: 15px; font-family: verdana ;height: 17px;">
                                            (Inc. of all Taxes) </div>
                                        <div
                                            style="text-align: left; font-size: 15px; font-weight: 600; font-family: verdana;margin-left: 15px;height: 18px;">
                                            <%=i.UniqueBarcode%>
                                        </div>
                                        <div
                                            style="text-align: left; font-size: 15px; font-weight: 600; font-family: verdana;margin-left: 15px;height: 25px;">
                                            <%=i.ProductNames%>
                                        </div>
                                    </th>

                                </tr>
                            </table> 
                            <%}%>
